<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<AnchorPane fx:id="root" minHeight="0.0" minWidth="0.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CircleChartController">
   <children>
      <VBox alignment="CENTER" minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="titleLabel" text="" wrapText="true"/>
            <StackPane fx:id="chartPane" minHeight="120.0" minWidth="120.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <PieChart fx:id="pieChart" minHeight="0.0" minWidth="0.0" />
                  <Circle fx:id="circle" fill="DODGERBLUE" stroke="BLACK" strokeType="INSIDE" />
                  <VBox alignment="CENTER" StackPane.alignment="CENTER">
                     <children>
                        <Label fx:id="gradeLabel" text="" minHeight="0.0" maxHeight="-Infinity" maxWidth="-Infinity" VBox.vgrow="ALWAYS"/>
                        <Label fx:id="percentageLabel" text="" minHeight="0.0" maxHeight="-Infinity" maxWidth="-Infinity" VBox.vgrow="ALWAYS"/>
                        <Label fx:id="fractionLabel" styleClass="fractionLabelColor" text="" minHeight="0.0" maxHeight="-Infinity" maxWidth="-Infinity" VBox.vgrow="ALWAYS"/>
                     </children>
                  </VBox>
               </children>
            </StackPane>

         </children>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@../../style/colors.css" />
      <URL value="@../../style/piechart.css" />
   </stylesheets>
</AnchorPane>
